                }
                else
                {

                    @*<li style="float:right" class="nav-item mx-0 mx-lg-1">*@<a class="nav-link py-3 px-0 px-lg-3 rounded" href="/Home/Index">LogIn <span><img src="~/images/nn2.png" /></span></a>@*</li>*@

