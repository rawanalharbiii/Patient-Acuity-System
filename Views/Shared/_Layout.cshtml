            <nav id="navbar" class="navbar order-last order-lg-0">
                <ul>
                    <li><a href="~/Home/welcom">Home</a></li>
                    <li><a href="~/ACUITY_EV/acuitylist">Acuity Tool </a></li>
                    <li><a href="~/Home/page1">Addmition Patients</a></li>
                    <li><a href="~/Home/ReviewTool">Acuity Review</a></li>
                    <li><a href="~/Home/UserDashBoard">Search </a></li>
                    @*<li><a href="~/portfolio.html">Portfolio</a></li>*@
                    <li><a href="~/Home/Contact">Contact Us</a></li>
                </ul>
                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav><!-- .navbar -->

            <div class="header-social-links">
                @if (Session["USER_NAME"] != null)
                {
                    @*<li class="nav-item mx-0 mx-lg-1">*@
                <a class="nav-link py-3 px-0 px-lg-3 rounded"href="/Home/Index">
                    Welcome @Session["USER_NAME"].ToString() <span><img src="~/images/nn2.png" /></span> &hearts;
                </a>
                 @*@Html.ActionLink("Log Off ", "LogOff", "Home", new { @class = "nav-link py-3 px-0 px-lg-3 rounded" })*@
